<div class="stats">
  <table>
    <tbody>
      <tr>
        <th>Tweetsss</th>
        <th>Followings</th>
        <th>Followers</th>
      </tr>
        <td><%= link_to @user.tweets.count, tweets_path %></td>
        <td><%= link_to @user.following.count, following_user_path(current_user) %></td>
        <td><%= link_to @user.followers.count, followers_user_path(current_user) %></td>
      </tr>
    </tbody>
  </table>
</div>